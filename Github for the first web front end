	<!-无序列表-->
	<ul>
	<li>item</li>
	<li>i</li>
	</ul>
	<!-有序列表-->
	<ol>
	<li>i</li>
	<li>i</li>
	</ol>
	<br>
	<!-表格-->
	<table border="2px">
	<caption>first</caption>
	<thead>
		<tr>
			<th>table head</th>
			<th>table head</th>
			<th>table head</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td>1999.8-2000.7</td>
			<td>xxxxxx</td>
			<td>职称</td>
		</tr>
	<tr><td>table data</td></tr>
	<tr><td>table data</td></tr>
	<tr><td>table data</td></tr>
	</tbody>
	</table>
	<br>
	<!-容器元素-->
	<div>div块级容器</div>
	<span>span行内容器</span>
	<form action="" method="post" enctype="multipart/form-data">
	<p>姓名：<input type="text" /></p>
	<p>简介：<textarea></textarea></p>
	<p>简历附件：<input type="file"/></p>
